<?xml version="1.0" encoding="UTF-8"?>
<view>
	<name>Default</name>
	<displayname>Blog Application View (Default)</displayname>
	<description>Blog Default View</description>
	<version>0.0.1</version>
	<repo>https://github.com/h-w-f/view-blog-default</repo>
	<dependencies>
		<application>
			<name>Blog</name>
			<version>0.0.1</version>
			<repo>https://github.com/h-w-f/application-blog</repo>
		</application>
	</dependencies>
	<settings>
		<cache>false</cache>
		<layout>/layout/view.html</layout>
		<head>
			<title>Blog - </title>
			<meta>
				<name>viewport</name>
				<content>width=device-width, initial-scale=1, shrink-to-fit=no</content>
			</meta>
			<meta>
				<name>description</name>
				<content>Blog Views</content>
			</meta>
			<meta>
				<name>author</name>
				<content>Guru</content>
			</meta>
			<meta>
				<name>keywords</name>
				<content>HTML, CSS, JavaScript</content>
			</meta>
			<link>
				<href>/views/Blog/Default/css/bootstrap.min.css</href>
			</link>
			<script>
				<src>/views/Blog/Default/js/jquery-3.5.1.min.js</src>
			</script>
			<script>
				<src>/views/Blog/Default/js/bootstrap.min.js</src>
			</script>
		</head>
		<body>
			<params>id="body" class="body"</params>
		</body>
		<footer>
			<params>class="footer"</params>
			<script>
				<src></src>
			</script>
		</footer>
	</settings>
</view>